<synthetic xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://www.xebialabs.com/deployit/synthetic" xsi:schemaLocation="http://www.xebialabs.com/deployit/synthetic synthetic.xsd">

    <type type="flowdock.configuration" extends="xlrelease.Configuration">
        <property name="apiUrl" category="input" label="API url" required="true" default="https://api.flowdock.com"/>
        <property name="apiKey" category="input" label="API key" required="true" />
        <property name="proxyHost" category="input" label="Proxy Host" required="false"/>
        <property name="proxyPort" category="input" label="Proxy Port" kind="integer" required="false"/>
        <property name="templateName" category="input" label="Template Name" kind="string" required="false"/>
        <property name="flowToken" category="input" label="Flow Token" kind="string" required="false"/>
        <property name="orgParamName" category="input" label="Organization/Parameterized Name" kind="string" required="false"/>
        <property name="enabled" category="input" label="Enabled" kind="boolean" />
    </type>
</synthetic>
